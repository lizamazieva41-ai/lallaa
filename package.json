{
  "name": "payment-sandbox-api",
  "version": "1.1.0",
  "description": "Payment Instrument Sandbox Platform - REST API for BIN lookup, IBAN validation, credit card generation, and test payment cards",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start:prod": "node dist/index.js",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint src/**/*.ts",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:prod": "npm run build && npm run build:migrate && npm run migrate:prod:run",
    "migrate:prod:run": "node dist/database/migrate.js",
    "seed": "ts-node src/database/seed.ts",
    "seed:force": "ts-node src/database/seed.ts --force-seed",
    "build:migrate": "tsc src/database/migrate.ts --outDir dist/database",
    "etl": "node ./node_modules/ts-node/dist/bin.js scripts/etl/etl.ts",
    "golden-set:generate": "node ./node_modules/ts-node/dist/bin.js scripts/golden-set/generate-from-sources.ts --min=5000 --mode=loose --fill=manual --output=golden-set.json",
    "golden-set:manual-sample": "node ./node_modules/ts-node/dist/bin.js scripts/golden-set/manual-verification.ts --input=data/golden-set/golden-set.json --count=500 --seed=1337",
    "accuracy:baseline": "node ./node_modules/ts-node/dist/bin.js scripts/accuracy/measureBaseline.ts --golden-set=golden-set.json --output=reports/accuracy/baseline-report.json",
    "accuracy:report": "node ./node_modules/ts-node/dist/bin.js scripts/accuracy/generateReport.ts --output=reports/accuracy/accuracy-report.json",
    "readiness:check": "node ./node_modules/ts-node/dist/bin.js scripts/readiness-assessment.ts",
    "countries:iso3166": "node ./node_modules/ts-node/dist/bin.js scripts/countries/import-iso3166.ts --output=iso3166-1.json",
    "fraud:train": "node ./node_modules/ts-node/dist/bin.js scripts/fraud/trainModel.ts",
    "etl:enrich-batch": "node ./node_modules/ts-node/dist/bin.js scripts/etl/enrich-batch.ts",
    "etl:rollback": "node ./node_modules/ts-node/dist/bin.js scripts/etl/rollback-version.ts",
    "etl:report-countries-banks": "npx ts-node scripts/etl/report-countries-banks.ts",
    "etl:report-filter-options": "npx ts-node scripts/etl/report-filter-options.ts",
    "etl:test-cards": "npx ts-node src/services/testCardsETL.ts",
    "start": "node dist/index.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "restart:pm2": "pm2 restart payment-sandbox-api",
    "stop:pm2": "pm2 stop payment-sandbox-api",
    "logs:pm2": "pm2 logs payment-sandbox-api",
    "status:pm2": "pm2 list",
    "deploy": "npm ci && npm run build && npm run migrate:prod && npm run seed && npm run start:pm2",
    "analyze-reports": "ts-node scripts/analyze-reports.ts",
    "analyze-reports:help": "ts-node scripts/analyze-reports.ts --help",
    "test-analysis": "ts-node scripts/test-analysis.ts",
    "verify-analysis": "ts-node scripts/verify-analysis.ts",
    "track-completion": "ts-node scripts/track-completion.ts",
    "partition:create": "ts-node scripts/create-monthly-partitions.ts create",
    "partition:archive": "ts-node scripts/create-monthly-partitions.ts archive",
    "partition:list": "ts-node scripts/create-monthly-partitions.ts list",
    "stats:aggregate": "ts-node scripts/card-statistics-aggregator.ts",
    "job:worker": "ts-node src/workers/cardGenerationWorker.ts",
    "uniqueness:cleanup": "ts-node scripts/uniqueness-pool-cleanup.ts",
    "bloom:sync": "ts-node scripts/bloom-filter-sync.ts",
    "cache:warm": "ts-node scripts/cache-warming.ts",
    "validate:uniqueness": "ts-node scripts/validate-uniqueness.ts",
    "load-test": "ts-node scripts/load-testing.ts",
    "migrate:data": "ts-node scripts/data-migration.ts",
    "migrate:rollback": "ts-node scripts/rollback-migration.ts"
  },
  "dependencies": {
    "@types/qrcode": "^1.5.6",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bloom-filters": "^3.0.4",
    "bull": "^4.12.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "credit-card-type": "^10.1.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pm2": "^6.0.14",
    "prom-client": "^15.1.0",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^9.0.1",
    "redis": "^4.6.12",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "world-countries": "^4.1.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "csv-parse": "^6.1.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
